<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.SSM.Mapper.ClasssMapper">
	 <select 	id="getClasss"  parameterType="com.SSM.Entity.Classs"  resultMap="classstMap">
	 select * from classs 
	 </select>
	 
	 
